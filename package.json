{
	"name": "workout-planner",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "supabase start && vite dev",
		"dev:mobile": "supabase start && vite dev --host 0.0.0.0",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:migration:new": "supabase migration new",
		"db:migration:auto": "supabase db diff --use-migra -f",
		"db:types": "supabase gen types typescript --local --schema public > src/lib/types/supabase.ts",
		"db:reset": "supabase db reset",
		"db:stop": "supabase stop"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.561.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^25.0.0",
		"@types/web-push": "^3.6.4",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"formsnap": "^2.0.1",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"sveltekit-superforms": "^2.28.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@neoconfetti/svelte": "^2.2.2",
		"@supabase/supabase-js": "^2.86.2",
		"web-push": "^3.6.7",
		"zod": "^4.1.13"
	}
}
